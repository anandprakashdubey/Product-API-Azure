{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ApimServiceName": {
      "type": "string",
      "metadata": {
        "description": "Name of the API Management"
      }
    },
    "LinkedTemplatesBaseUrl": {
      "type": "string",
      "metadata": {
        "description": "Base URL of the repository"
      }
    }
  },
  "resources": [
    {
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('LinkedTemplatesBaseUrl'), 'CreaterFiles/ProductAPIMService-Dev-master.template.json')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "ApimServiceName": {
            "value": "[parameters('ApimServiceName')]"
          },
          "LinkedTemplatesBaseUrl": {
            "value": "[concat(parameters('LinkedTemplatesBaseUrl'),'/CreaterFiles']"
          }
        }
      },
      "name": "ProductAPIVersionSet",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "dependsOn": []
    }
  ]
}
